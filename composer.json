{
  "name": "soluciones-ypunto/cake-admin",
  "description": "CakePHP admin plugin",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "cakephp/cakephp": "^3.5",
    "cakephp/bake": "^1.9",
    "friendsofcake/bootstrap-ui": "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.14|^6.0",
    "cakephp/debug_kit": "^3.17.0",
    "cakephp/migrations": "^2.0.0",
    "dereuromark/cakephp-ide-helper": "0.*",
    "mobiledetect/mobiledetectlib": "2.*",
    "xavier83ar/cakephp-spanish-inflections": "^1.0",
    "symfony/var-dumper": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Ypunto\\Admin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/DemoApp/src/",
      "Ypunto\\Admin\\Test\\": "tests/",
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
    }
  }
}
